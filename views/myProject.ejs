<!doctype html>
<html lang="en">
<%-include('./partials/head');%>

  <body>
<div class="container">
    <div class="row">
      <%for (var i of data){%>
        <div class="col">
          <div class="card my-2" style="width: 18rem;">
            <img src="/public/projectImages/<%=i.img%>" class="card-img-top" alt="..."
              style="width: auto;height:150px">
            <div class="card-body">
              <h5 class="card-title">
                <%=i.title%>
              </h5>
              <p id="text" class="card-text">
                <%=i.description.substring(0,50)%>..
              </p>
              <a href="#" class="btn btn-primary">Read more</a>
            </div>
          </div>
        </div>
        <%}%>
    </div>
  </div>
  <%- include('./partials/footer.ejs'); %>


</body>

</html>